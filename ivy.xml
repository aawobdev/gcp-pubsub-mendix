<ivy-module version="2.0">
    <info organisation="com.mendix" module="gcpiot"/>
    <configurations defaultconfmapping="default">
        <conf name="default" description="run in modeler" extends="export"/>
        <conf name="export" description="connector export dependencies only"/>
    </configurations>
    <dependencies>
		<!-- for google pub/sub -->
		<dependency org="com.google.cloud" name="google-cloud-pubsub" rev="1.55.0"/>
		<!-- for google iot -->
		<dependency org="com.google.cloud" name="google-cloud-iot" rev="0.77.0-beta"/>
    </dependencies>
</ivy-module>
